{
  "name": "g-json-view",
  "version": "1.0.2",
  "description": "A Vue component for rendering JSON data as a tree structure.",
  "author": "GoodwinGuo <1065102410@qq.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:GoodwinGuo/g-json-view.git"
  },
  "homepage": "https://GoodwinGuo.github.io/g-json-view",
  "keywords": [
    "vue",
    "json",
    "format",
    "json-view",
    "vue-component",
    "vue-json-view",
    "g-json-view",
    "json-view"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GoodwinGuo/g-json-view/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/g-json-view.umd.cjs",
  "module": "./dist/g-json-view.js",
  "exports": {
    ".": {
      "import": "./dist/g-json-view.umd.cjs",
      "require": "./dist/g-json-view.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/core": "^19.3.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.1.0",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  }
}
